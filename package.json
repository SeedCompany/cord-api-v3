  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@seedcompany/eslint-plugin": "^3.3.0",
    "@types/common-tags": "^1.8.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/human-format": "^1.0.0",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.11",
    "@types/pako": "^2.0.0",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.26",
    "@types/stack-trace": "^0.0.30",
    "@types/triple-beam": "^1.3.2",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/parser": "*",
    "eslint": "^8.29.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^13.1.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.0.2",
    "type-fest": "^2.19.0",
    "typescript": "^4.9.3",
    "typescript-transform-paths": "^3.4.4"
  },
  "resolutions": {
    "@nestjs/cli/typescript": "^4.8.2"
  },
  "dependenciesMeta": {
    "@apollo/protobufjs": {
      "built": false
    },
    "@nestjs/core": {
      "built": false
    },
    "core-js-pure": {
      "built": false
    },
    "typescript": {
      "unplugged": true
    }
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "./src/polyfills.ts"
    ],
    "testEnvironment": "node"
  }
}
