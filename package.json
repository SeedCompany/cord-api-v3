    "highlightjs-cypher": "^1.1.5",
    "human-format": "^1.2.0",
    "ioredis": "^5.3.2",
    "iso-3166-1": "^2.1.1",
    "ix": "^5.0.0",
    "js-levenshtein-esm": "^1.2.0",
    "jsonwebtoken": "^9.0.1",
    "lazy-get-decorator": "^2.2.1",
