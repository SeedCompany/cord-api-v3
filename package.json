{
  "name": "cord-api-3-v3",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docker:build": "scripts/docker-build.sh",
    "docker:run": "scripts/docker-run.sh",
    "docker:test": "scripts/docker-run.sh yarn test:e2e --runInBand",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts,.tsx --fix --max-warnings 0 .",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "type-check": "tsc -p tsconfig.check.json"
  },
  "dependencies": {
    "@golevelup/nestjs-discovery": "^2.2.3",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.7.2",
    "apollo-server-core": "^2.9.16",
    "apollo-server-express": "^2.9.15",
    "argon2": "^0.26.1",
    "aws-sdk": "^2.608.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "common-tags": "^1.8.0",
    "cypher-query-builder": "^5.0.4",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "faker": "^4.1.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lazy-get-decorator": "^2.2.0",
    "lodash": "^4.17.15",
    "luxon": "^1.21.3",
    "neo4j-driver": "1.7.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "shortid": "^2.2.15",
    "source-map-support": "^0.5.16",
    "type-graphql": "^0.17.6",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/common-tags": "^1.8.0",
    "@types/express": "^4.17.1",
    "@types/faker": "^4.1.9",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.1.4",
    "@types/js-yaml": "^3.12.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.21.0",
    "@types/node": "^13.9.1",
    "@types/passport-local": "^1.0.33",
    "@types/shortid": "^0.0.29",
    "@types/stack-trace": "^0.0.29",
    "@types/supertest": "^2.0.8",
    "@types/triple-beam": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "apollo-server-testing": "^2.9.16",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-local-rules": "^0.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.12.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "{src,tools}/**/*.{ts,tsx}": [
      "eslint --ext .ts,.tsx --fix --max-warnings 0",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
