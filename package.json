{
  "name": "cord-api-v3",
  "version": "3.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=12.17 || >=14"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "console": "node -r ./tools/ts-node-register src/console.ts",
    "console:prod": "node dist/console.js",
    "docker:build": "scripts/docker-build.sh",
    "docker:run": "scripts/docker-run.sh",
    "docker:test": "scripts/docker-run.sh yarn test:e2e --runInBand",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts,.tsx --fix --max-warnings 0 .",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest -i --config ./test/jest-e2e.json",
    "test:security": "jest -i --config ./test/jest-security.json",
    "madge": "madge --extensions ts,tsx --ts-config=tsconfig.json src",
    "type-check": "tsc -p tsconfig.check.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.45.0",
    "@aws-sdk/s3-request-presigner": "^3.45.0",
    "@golevelup/nestjs-discovery": "^2.3.1",
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/cqrs": "^8.0.1",
    "@nestjs/graphql": "patch:@nestjs/graphql@9.1.2#./patches/nestjs-graphql.patch",
    "@nestjs/platform-express": "^8.2.4",
    "@patarapolw/prettyprint": "^1.0.3",
    "@seedcompany/nestjs-email": "^2.0.5",
    "apollo-server-core": "^3.6.1",
    "apollo-server-express": "^3.6.1",
    "argon2": "^0.28.3",
    "aws-xray-sdk-core": "^3.3.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cli-highlight": "^2.1.11",
    "commander": "^8.3.0",
    "common-tags": "^1.8.2",
    "cookie-parser": "^1.4.6",
    "cypher-query-builder": "patch:cypher-query-builder@6.0.4#./patches/cypher-query-builder.patch",
    "dataloader": "^2.0.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "execa": "^5.1.1",
    "express": "^4.0.0",
    "faker": "^5.5.3",
    "fast-safe-stringify": "^2.1.1",
    "got": "^11.8.2",
    "graphql": "^15.8.0",
    "highlightjs-cypher": "^1.1.1",
    "iso-3166-1": "^2.1.1",
    "ix": "^4.5.2",
    "js-levenshtein-esm": "^1.2.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lazy-get-decorator": "^2.2.0",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "nanoid": "^3.1.30",
    "neo4j-driver": "^4.4.1",
    "nestjs-console": "^7.0.1",
    "p-retry": "^4.6.1",
    "pkg-up": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.1",
    "source-map-support": "^0.5.21",
    "ts-essentials": "^9.1.2",
    "ts-transformer-keys": "^0.4.3",
    "winston": "^3.3.3",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.6",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.4",
    "@seedcompany/eslint-plugin": "^1.8.0",
    "@types/common-tags": "^1.8.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.27",
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.8",
    "@types/mjml-react": "^2.0.2",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/stack-trace": "^0.0.29",
    "@types/triple-beam": "^1.3.2",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "madge": "^5.0.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "type-fest": "^2.9.0",
    "typescript": "4.5.*"
  },
  "resolutions": {
    "@nestjs/graphql/@nestjs/mapped-types": "^1.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "astTransformers": {
          "before": [
            "./tools/jest-ts-transformer-keys"
          ]
        }
      }
    }
  }
}
