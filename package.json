    "docker:run": "scripts/docker-run.sh",
    "docker:test": "scripts/docker-run.sh yarn test:e2e --runInBand",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts,.tsx --fix --max-warnings 0 .",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest -i --config ./test/jest-e2e.json",
    "madge": "madge --extensions ts,tsx --ts-config=tsconfig.json src",
    "type-check": "tsc -p tsconfig.check.json"
  },
