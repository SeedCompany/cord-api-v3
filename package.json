    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.16.19",
    "@types/pako": "^2.0.0",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.15",
    "@types/stack-trace": "^0.0.30",
    "@types/triple-beam": "^1.3.2",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.45.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "husky": "^4.3.8",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.1",
    "ts-transformer-keys": "^0.4.4",
    "type-fest": "^4.0.0",
    "typescript": "^5.1.6",
    "typescript-transform-paths": "^3.4.6"
  },
  "resolutions": {
    "cypher-query-builder/neo4j-driver": "^5.9.0",
    "@nestjs/cli/fork-ts-checker-webpack-plugin": "npm:lodash@*",
    "@nestjs/cli/webpack": "npm:lodash@*",
    "@nestjs/cli/typescript": "^5.1.6"
  },
  "dependenciesMeta": {
    "@apollo/protobufjs": {
      "built": false
    },
    "@nestjs/core": {
      "built": false
    },
    "core-js-pure": {
      "built": false
    },
    "typescript": {
      "unplugged": true
    }
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "./src/polyfills.ts"
    ],
    "testEnvironment": "node"
  }
}
