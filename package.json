    "js-levenshtein-esm": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "lazy-get-decorator": "^2.2.1",
    "lodash": "^4.17.21",
    "lru-cache": "^7.14.1",
    "luxon": "^3.1.1",
    "nanoid": "^3.3.4",
    "neo4j-driver": "^4.4.7",
